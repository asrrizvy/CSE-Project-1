#include <stdio.h>
int main() {
    int start, end, sum = 0, i;
    printf("Enter two integers: ");
    scanf("%d %d", &start, &end);

    printf("Sum of numbers from %d to %d = ", start, end);
    for (i = start; i <= end; i++) {
        sum += i;
        if (i != end)
            printf("%d + ", i);
        else
            printf("%d", i);
    }
    printf(" = %d\n", sum);

    return 0;
}
